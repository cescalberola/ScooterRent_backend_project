const { StoreScooter } = require("../models/index.js");

const StoreScooterController = {
  async create(req, res) {
    try {
      console.log("object");
      const storeScooter = await StoreScooter.create(req.body);
      res.status(201).send({ message: 'StoreScooter created successfully', storeScooter });
    } catch (err) {
      console.error(err);
      res.status(500).send(err);
    }
  }
};

module.exports = StoreScooterController
